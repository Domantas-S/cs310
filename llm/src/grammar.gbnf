root ::= record
treatment ::= "{"   (ws   "\"Drug\":"   ws   common   ",")?   (ws   "\"Disorder\":"   ws   common   ",")?   (ws   "\"Dosage\":"   ws   common   ",")?   (ws   "\"Duration\":"   ws   common   ",")?   (ws   "\"Trigger\":"   ws   common   ",")?   (ws   "\"Route\":"   ws   common   ",")?   (ws   "\"Time_elapsed\":"   ws   common   ",")?   (ws   "\"Freq\":"   ws   common   ",")?   (ws   "\"Combination\":"   ws   combinationlist)?   "}"
combination ::= "{"   (ws   "\"Drug\":"   ws   common   ",")?   (ws   "\"Trigger\":"   ws   common)?   "}"
combinationlist ::= "[]" | "["   ws   combination   (","   ws   combination)*   "]"
subject ::= "{"   (ws   "\"Age\":"   ws   common   ",")?   (ws   "\"Disorder\":"   ws   common   ",")?   (ws   "\"Gender\":"   ws   common   ",")?   (ws   "\"Population\":"   ws   common   ",")?   (ws   "\"Race\":"   ws   common)?   "}"
valuedcommonstr ::= "{"   ws   "\"value\":"   ws   string   "}"
valuedcommonbool ::= "{"   ws   "\"value\":"   ws   boolean   "}"
common ::= "{"   ws   "\"text\":"   ws   stringlist   ","   ws   "\"start\":"   ws   numberlist   ","   ws   "\"entity_id\":"   ws   stringlist   "}"
eventinfo ::= "{"   ws   "\"event_type\":"   ws   string   ","   ws   "\"event_id\":"   ws   string   "}"
event ::= "{"   (ws   "\"Effect\":"   ws   common   ",")?   (ws   "\"Trigger\":"   ws   common   ",")?   (ws   "\"Negated\":"   ws   valuedcommonbool   ",")?   (ws   "\"Severity\":"   ws   valuedcommonstr   ",")?   (ws   "\"Speculated\":"   ws   valuedcommonbool   ",")?   (ws   "\"Subject\":"   ws   subject   ",")?   (ws   "\"Treatment\":"   ws   treatment)?   "}"
eventlist ::= "[{"   "\"events\":"  ws   "["   ws   event   (","   ws   event)*   "]}]"
record ::= "{"   ws   "\"id\":"   ws   string   ","   ws   "\"context\":"   ws   string   ","   ws   "\"is_mult_event\":"   ws   boolean   ","   ws   "\"annotations\":"   ws   eventlist   "}"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \n\t]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   number   (","   ws   number)*   ws   "]"
